{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "node index.js",
    "lint": "eslint .",
    "test": "jest",
    "dev": "npm-run-all -p \"build:client -- --watch\" \"build:server -- --watch\" dev:server",
    "dev:server": "nodemon --watch server --exec \"node index.js\"",
    "build": "run-s build:client build:server",
    "build:production": "NODE_ENV=production run-s \"build:client -- -p\" \"build:server\"",
    "build:client": "webpack --config webpack.client.js",
    "build:server": "webpack --config webpack.server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jnmorse/udemy-react-ssr.git"
  },
  "keywords": [
    "react",
    "ssr"
  ],
  "author": "Joseph Morse <tamed.lionheart@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jnmorse/udemy-react-ssr/issues"
  },
  "homepage": "https://github.com/jnmorse/udemy-react-ssr#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "express-http-proxy": "^1.0.7",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "serialize-javascript": "^1.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "concurrently": "^3.5.0",
    "dynamic-cdn-webpack-plugin": "^3.4.0",
    "eslint": "^4.9.0",
    "eslint-config-jnmorse": "^1.1.12",
    "module-to-cdn": "^3.1.1",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "webpack": "^3.8.1",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-node-externals": "^1.6.0"
  }
}
